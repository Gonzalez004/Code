#include <allegro5/allegro.h>
#include <iostream>
using namespace std;
int main() {
  cout << "Hello World" << endl;
  ALLEGRO_DISPLAY *display = NULL;
	al_init();
	display = al_create_display(900, 900);
	al_clear_to_color(al_map_rgb(55, 12, 85));
	al_flip_display();
	al_rest(10.0);
	al_destroy_display(display);
	return 0;
}
