#include <iostream>
using namespace std;
int inventory[10];
int arrow;
int sum = 0;
int main() {
	for (int i = 0; i <= 10; i++) {
		cout << "Put in 10 integers Please. Por Favor" << (i + 1) << endl;
		cin >> arrow;
		inventory[i] = arrow;
	}
	for (int j = 0; j <= 10; j++) {
		if (inventory[j] >= 10) {
			sum++;
		}
	}
	cout << "The amount of intergers that are greater than or equal to 10 is " << sum << endl;
	system("pause");
	return 0;
}
