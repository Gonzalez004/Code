#include <iostream>
using namespace std;
int inventory[10];
int arrow;
int sum = 0;
int main() {
	cout << "Put in your number please Please. Por Favor" << endl;
	cout << "Fill each slot with a number" << endl;
	for (int i = 0; i <= 9; i++) {
		i + 1;
		cin >> arrow;
		inventory[i] = arrow;
	}
	for (int i = 0; i <= 9; i++) {
		cout << inventory[i];
	}
	cout << endl;
	system("pause");
}
