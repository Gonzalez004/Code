#include <iostream>
using namespace std;

int main() {
	int x;
	int sum = 0; 

	for (int x = 3; x < 1000; x++) {
		if (x % 3 == 0) {
			sum += x;
			cout << x << "  " ;
		}
		
		if ((x % 3 == 0) && (x % 5 == 0))
			sum -= x;
			
			if (x % 5 == 0) {
			sum += x;
			cout << x << "  " ;
		}
	}
	cout << endl;
	cout << "Total Sum: " << sum << endl;
	return 0;
}
